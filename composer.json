{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "psr/http-message": "^1.1.0",
    "laminas/laminas-diactoros": "^3.3",
    "laminas/laminas-httphandlerrunner": "^2.10",
    "aura/router": "^3.3.0",
    "ext-mysqli": "*",
    "php-di/php-di": "^7.0",
    "vlucas/phpdotenv": "^5.6",
    "symfony/console": "^6.4",
    "ext-pdo": "*"
